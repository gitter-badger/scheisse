<xs:schema xmlns:tns="http://service.scheisse_server.games.dedda.org/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           version="1.0" targetNamespace="http://service.scheisse_server.games.dedda.org/">

    <xs:element name="changePassword" type="tns:changePassword"/>

    <xs:element name="changePasswordResponse" type="tns:changePasswordResponse"/>

    <xs:element name="getContacts" type="tns:getContacts"/>

    <xs:element name="getContactsResponse" type="tns:getContactsResponse"/>

    <xs:element name="getInventoryFor" type="tns:getInventoryFor"/>

    <xs:element name="getInventoryForResponse" type="tns:getInventoryForResponse"/>

    <xs:element name="getUser" type="tns:getUser"/>

    <xs:element name="getUserResponse" type="tns:getUserResponse"/>

    <xs:element name="getUserWithPassword" type="tns:getUserWithPassword"/>

    <xs:element name="getUserWithPasswordResponse" type="tns:getUserWithPasswordResponse"/>

    <xs:complexType name="getInventoryFor">
        <xs:sequence>
            <xs:element name="arg0" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="getInventoryForResponse">
        <xs:sequence>
            <xs:element name="return" type="tns:inventoryContainer" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="inventoryContainer">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="size" type="xs:long"/>
            <xs:element name="userId" type="xs:long"/>
            <xs:element name="slots" type="tns:slotContainer" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="slotContainer">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="amount" type="xs:long"/>
            <xs:element name="item" type="tns:itemContainer" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="itemContainer">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="value" type="xs:long"/>
            <xs:element name="name" type="xs:string" minOccurs="0"/>
            <xs:element name="type" type="xs:string" minOccurs="0"/>
            <xs:element name="attack" type="xs:long"/>
            <xs:element name="armor" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="getUserWithPassword">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="password" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="getUserWithPasswordResponse">
        <xs:sequence>
            <xs:element name="return" type="tns:userContainer" minOccurs="0"/>
